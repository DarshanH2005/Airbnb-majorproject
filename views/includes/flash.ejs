<% if(success && success.length){ %>
    <div class="alert alert-success alert-dismissable fade show col-6 offset-3 " role="alert" style="display: flex;">
        <%= success  %>
        
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close" style="position: relative; align-items: end;"></button>
</div>
<% } %>